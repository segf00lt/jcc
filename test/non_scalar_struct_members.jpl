struct Bob {
    i: int = 11;
    c: char = 27;
}

struct Alice {
    id: u16 = 255;
}

struct Another_Alice {
    id: u16 = 255;
}

struct Jane {
    using Bob;
    using Alice;

    q: struct {
        r: f64;
        i: f64;
    }
    f: float;
}

struct Type_info_int {
    using Type_info;
    bits: u8;
    sign: bool;
}

struct Type_info {
    tag: u64;
}

struct Type_info_float {
    using Type_info;
    bits: u8;
}

struct Type_info_pointer {
    using Type_info;
    pointer_to: *Type_info;
}

struct Type_info_struct {
    using Type_info;

    members:   []*Type_info_member;
    name:      []char;
}

struct Type_info_member {
    using Type_info;

    name:    []char;
    type:     *Type_info;
    offset:    u64;
}

proc main() {
    test: Type_info_struct;
}
